<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Rocket League Cards</title>
    <link rel="stylesheet" href="../css/style.css">
    <script src="./js/index.js" defer></script>
</head>

<body>
    <%- include('sidebar') %>
        <section class="mainContent">
            <h1>All Playstyles:</h1>
            <section class="classesDisplay">

                <% playstyles.forEach(playstyle=> { %>

                    <p><a href="./playstyle/<%= playstyle %>">
                            <%= playstyle %>
                        </a></p>
                    <% }) %>

            </section>

        </section>
</body>

</html>